<view class='main'>
  <view class='user-card-warpper'>
    <view class='avatar'>
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <view class='username'>
        <open-data class="open-username" type="userNickName"></open-data>
    </view>
    <view class='userinfo-group'>
      <view class='true'>正确率:{{right_num}}%</view>
      <view class='false'>错误率:{{wrong_num}}%</view>
      <view class='yizu'>已作答:{{user_subject_sum_nums}}题</view>
    </view>
  </view>

  <view class='user-info-wrapper'>
  <canvas canvas-id="canvas1" disable-scroll="true" class="lineCanvas"></canvas>
    <view class='user-exam-items'>
      <text>我的错题</text>
    </view>
    <view class='user-exam-items'>
      <text>我的收藏</text>
    </view>

  </view>
    




</view>
